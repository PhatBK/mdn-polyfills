{
  "name": "mdn-polyfills",
  "version": "1.0.0",
  "description": "MDN polyfills - Array.from, Array.find, Object.assign, ...",
  "scripts": {
    "transpile": "babel ./src --out-dir ./dist",
    "prepublish": "npm run transpile && webpack",
    "test": "ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/msn0/mdn-polyfills.git"
  },
  "keywords": [
    "Array.prototype.from",
    "Array.prototype.find",
    "Object.assign"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/msn0/mdn-polyfills/issues"
  },
  "homepage": "https://github.com/msn0/mdn-polyfills#readme",
  "devDependencies": {
    "ava": "^0.16.0",
    "babel-cli": "^6.10.1",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "webpack": "^1.13.1"
  },
  "ava": {
    "files": [
      "src/**/spec.js"
    ],
    "source": [
      "src/**/*.js"
    ],
    "concurrency": 5,
    "failFast": true,
    "powerAssert": true,
    "require": [
      "babel-register",
      "./setup-browser-env.js"
    ],
    "babel": "inherit"
  }
}
